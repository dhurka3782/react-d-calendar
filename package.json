{
  "name": "react-d-calendar",
  "version": "1.0.1",
  "description": "A fully customizable, accessible, and responsive React calendar component",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/types.d.ts"
    },
    "./dist/styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  },
  "scripts": {
    "test": "jest",
    "build": "tsc --emitDeclarationOnly && webpack --mode production",
    "start": "webpack serve --open",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "format": "prettier --write src/**/*.{js,ts,tsx,css}"
  },
  "keywords": [
    "react",
    "calendar",
    "date-picker",
    "range-selection",
    "accessible",
    "customizable",
    "event-calendar"
  ],
  "author": "Dhurka D <dhurka3782@example.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "lucide-react": "^0.441.0 || ^0.511.0",
    "react-window": "^1.8.10 || ^1.8.11"
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.6",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.511.0",
    "prop-types": "^15.8.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.5",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.3.3",
    "style-loader": "^4.0.0",
    "typescript": "^5.5.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhurka3782/react-d-calendar.git"
  },
  "homepage": "https://github.com/dhurka3782/react-d-calendar#readme",
  "bugs": {
    "url": "https://github.com/dhurka3782/react-d-calendar/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css"
  ]
